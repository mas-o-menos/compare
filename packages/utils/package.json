{
  "name": "@bundle-stats/utils",
  "version": "1.0.0-rc.0",
  "description": "BundleStats utils",
  "repository": "bundle-stats/bundle-stats",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "engines": {
    "node": ">= 8.0"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run build-lib && npm run build-esm",
    "build-lib": "rm -fr ./lib && babel -D src -d lib --ignore **/__tests__/** --presets=@babel/preset-env",
    "build-esm": "rm -fr ./lib-esm && babel -D src -d lib-esm --ignore **/__tests__/**",
    "lint": "eslint ./",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "test": "jest src"
  },
  "keywords": [],
  "author": {
    "name": "Viorel Cojocaru",
    "email": "vio@beanon.com",
    "url": "https://beanon.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bundle-stats/bundle-stats/issues"
  },
  "homepage": "https://github.com/bundle-stats/bundle-stats/blob/master/packages/utils/README.md",
  "dependencies": {
    "convert-units": "^2.3.4"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-plugin-lodash": "3.3.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "lodash": "4.17.11",
    "regenerator-runtime": "0.13.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.11"
  }
}
