{
  "name": "@bundle-stats/utils",
  "version": "2.8.0",
  "description": "BundleStats utils",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">= 10.0"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run build-lib && npm run build-esm",
    "build-lib": "rm -fr ./lib && npx tsc --rootDir src --outDir lib",
    "build-esm": "rm -fr ./lib-esm && rm -fr ./types && npx tsc --rootDir src --project tsconfig.esm.json --outDir lib-esm",
    "lint": "../../node_modules/.bin/eslint . --ext .js",
    "test": "TZ=UTC jest src --maxWorkers=4"
  },
  "keywords": [],
  "author": {
    "name": "Viorel Cojocaru",
    "email": "vio@beanon.com",
    "url": "https://beanon.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relative-ci/bundle-stats.git"
  },
  "bugs": {
    "url": "https://github.com/relative-ci/bundle-stats/issues"
  },
  "homepage": "https://github.com/relative-ci/bundle-stats/blob/master/packages/utils#readme",
  "dependencies": {
    "convert-units": "^2.3.4",
    "query-string": "^6.14.1",
    "serialize-query-params": "^1.3.1",
    "superstruct": "^0.8.3"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/jest": "^26.0.22",
    "core-js": "3.10.1",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "jest-date-mock": "1.0.8",
    "lodash": "4.17.21",
    "regenerator-runtime": "0.13.7",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "lodash": "^4.17.11"
  }
}
