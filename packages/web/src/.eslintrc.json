{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": false,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "parser": "babel-eslint",
  "rules": {
    "react/no-unknown-property": "off",
    "react/react-in-jsx-scope": "off",
    "react/sort-comp": "off",
    "import/no-extraneous-dependencies": ["error"]
  }
}
